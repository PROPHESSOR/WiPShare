<?xml version='1.0' encoding='utf-8'?>
<widget id="com.prophessor.wipshare" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WifiWizard2">
        <param name="android-package" value="wifiwizard2.WifiWizard2" />
        <param name="onload" value="true" />
    </feature>
    <name>WiPShare</name>
    <description>
        An evolutionary app :)
    </description>
    <author email="xenteze@gmail.com" href="https://github.com/PROPHESSOR">
        PROPHESSOR
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
